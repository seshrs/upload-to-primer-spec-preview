{
  "name": "upload-to-primer-spec-preview",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Action to upload a Jekyll site preview to Primer Spec Preview",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seshrs/upload-to-primer-spec-preview.git"
  },
  "author": "Sesh Sadasivam",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^4.0.0",
    "@octokit/webhooks": "^7.21.0",
    "execa": "^5.1.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.19",
    "@typescript-eslint/parser": "^4.29.1",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.1.5",
    "eslint-plugin-jest": "^25.2.3",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.1.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.3.5"
  }
}
